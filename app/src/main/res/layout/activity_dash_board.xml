<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashBoard">


    <TextView
        android:id="@+id/welcomeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gontserratbold"
        android:text="Welcome "
        android:textSize="25sp"
        android:textColor="@color/back_blue"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/usernameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/welcomeTextView"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/gontserratbold"
        android:hint="Username"
        android:textAlignment="center"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/emailTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/usernameTextView"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/gontserratbold"
        android:hint="Email"
        android:textAlignment="center"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/FirstNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/emailTextView"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/gontserratbold"
        android:hint="FirstName"
        android:textAlignment="center"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/lastNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/FirstNameTextView"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/gontserratbold"
        android:hint="LastName"
        android:textAlignment="center"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/omangTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lastNameTextView"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="Omang" />

    <TextView
        android:id="@+id/genderTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/omangTextView"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="Gender" />

    <TextView
        android:id="@+id/DOBTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/genderTextView"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="DOB" />

    <TextView
        android:id="@+id/phoneNumberTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/DOBTextView"
        android:layout_marginTop="11dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="phoneNumber" />

    <TextView
        android:id="@+id/documentIDTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/phoneNumberTextView"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="DocumentID" />

    <TextView
        android:id="@+id/countryTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/documentIDTextView"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/gontserratbold"
        android:hint="Country"
        android:textAlignment="center"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/physicalAddressTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/countryTextView"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="PhysicalAddress" />

    <TextView
        android:id="@+id/postalAddressTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/physicalAddressTextView"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:hint="PostalAddress" />

    <Button
        android:id="@+id/logOutBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/postalAddressTextView"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/gontserratbold"
        android:textAlignment="center"
        android:backgroundTint="@color/back_blue"
        android:textAllCaps="false"
        android:text="LogOut" />


</RelativeLayout>